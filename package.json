    "@chainlink/contracts": "^0.1.6",
    "@libertypie/truffle-seeder": "^1.1.8",
    "@openzeppelin/contracts": "^3.4.0",
    "@truffle/hdwallet-provider": "^1.1.1",
    "argparse": "^2.0.1",
    "colors": "^1.4.0",
    "ethers": "^5.0.32",
    "slugify": "^1.5.0",
